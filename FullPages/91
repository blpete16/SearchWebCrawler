		$('img.moreNewsActive').css('visibility','visible');
		$('img.moreNewsInactive').css('visibility','hidden');
	}
	function moreNewsInactive() {
		$('img.moreNewsActive').css('visibility','hidden');
		$('img.moreNewsInactive').css('visibility','visible');
	}



